<?xml version="1.0" encoding="UTF-8"?>

<registry>
	<name>MUIS Test</name>
	<description>
		This toolkit provides access to code used for testing MUIS functionality
	</description>
	<version>15Jun2012</version>
	<dependencies>
		<classpath>classes</classpath>
		<depends>../base/MuisRegistry.xml</depends>
	</dependencies>
	<types>
		<type tag="paint">org.muis.test.widget.PaintWidget</type>
	</types>
	<resources>
		<resource tag="hello-world">src/main/resources/helloWorld.jpg</resource>
		<resource tag="big-animation">src/main/resources/BigAnimation.gif</resource>
		<resource tag="fire">src/main/resources/fire.gif</resource>
		<resource tag="waterfall">src/main/resources/waterfall.gif</resource>
		<resource tag="plant">src/main/resources/plant.gif</resource>
	</resources>
	<security />
</registry>

<?xml version="1.0" encoding="UTF-8"?>

<registry>
	<name>MUIS Base</name>
	<description>
		This toolkit provides basic MUIS functionality
	</description>
	<version>10Jul2012</version>
	<dependencies>
		<classpath>classes</classpath>
	</dependencies>
	<types>
        <type tag="block">org.muis.base.widget.Block</type>
        <type tag="image">org.muis.base.widget.Image</type>
        <type tag="button">org.muis.base.widget.Button</type>
        <type tag="simple">org.muis.base.layout.SimpleLayout</type>
        <type tag="box">org.muis.base.layout.BoxLayout</type>
        <type tag="raised-round">org.muis.base.style.RaisedRoundTexture</type>
        <type tag="raised-square">org.muis.base.style.RaisedSquareTexture</type>
	</types>
	<resources>
		<resource tag="img-load-icon">src/main/resources/imageLoading.gif</resource>
		<resource tag="img-load-failed-icon">src/main/resources/imageLoadFailed.png</resource>
	</resources>
	<security />
</registry>

<?xml version="1.0" encoding="UTF-8"?>

<!-- Color parsing grammar ->
<grammar>
	<select>
		<option>
			<literal storeAs="rgb">#</literal>
		</option>
		<option>
			<literal storeAs="hsb">$</literal>
		</option>
	</select>
	<charset pattern="[0-9][A-F][a-f]{6}" storeAs="value" />
</grammar>
